/* pages/dangerArea/dangerArea.wxss */
.dangerArea{
  display: flex;
  flex-flow: column nowrap;
}
.explain{
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-flow: row wrap;
  box-shadow: 0px 5px 20px #aaa;
  z-index: 999;
}
.explain view{
  background-color: #f8f8f8;
  width: 50%;
  text-align: center;
  line-height: 120rpx;
  font-size: 36rpx;
}
.map{
  width: 100%;
  flex: 1;
}






